{
  "$schema": "http://json-schema.org/draft-04/schema",
  "id": "http://example.com/example.json",
  "type": "object",
  "required": [
    "data"
  ],
  "properties": {
    "data": {
      "id": "#/properties/data",
      "type": "array",
      "additionalItems": false,
      "items": {
        "id": "#/properties/data/items",
        "type": "object",
        "required": [
          "id",
          "type",
          "attributes"
        ],
        "properties": {
          "id": {
            "id": "#/properties/data/items/properties/id",
            "type": "string"
          },
          "type": {
            "id": "#/properties/data/items/properties/type",
            "type": "string"
          },
          "attributes": {
            "id": "#/properties/data/items/properties/attributes",
            "type": "object",
            "required": [
              "cvxCode",
              "date",
              "groupName",
              "manufacturer",
              "note",
              "shortDescription"
            ],
            "properties": {
              "cvxCode": {
                "id": "#/properties/data/items/properties/attributes/properties/cvxCode",
                "type": "integer"
              },
              "date": {
                "id": "#/properties/data/items/properties/attributes/properties/date",
                "type": "string"
              },
              "groupName": {
                "id": "#/properties/data/items/properties/attributes/properties/groupName",
                "type": "string"
              },
              "manufacturer": {
                "id": "#/properties/data/items/properties/attributes/properties/manufacturer",
                "type": "null"
              },
              "note": {
                "id": "#/properties/data/items/properties/attributes/properties/note",
                "type": "string"
              },
              "shortDescription": {
                "id": "#/properties/data/items/properties/attributes/properties/shortDescription",
                "type": "string"
              }
            },
            "additionalProperties": false
          }
        },
        "additionalProperties": false
      }
    }
  },
  "additionalProperties": false
}
