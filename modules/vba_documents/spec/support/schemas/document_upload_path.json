{
  "schema": {
    "type": "object",
    "properties": {
      "data": {
        "type": "object",
        "properties": {
          "type": {
            "const": "document_upload"
          },
          "id": {
            "type": "uuidstring",
            "pattern": "^[0-9a-fA-F]{8}(-[0-9a-fA-F]{4}){3}-[0-9a-fA-F]{12}$"
          },
          "attributes": {
            "type": "object",
            "properties": {
              "status": {
                "type": "string",
                "enum": [
                  "pending",
                  "uploaded",
                  "received",
                  "processing",
                  "success",
                  "vbms",
                  "error"
                ]
              },
              "code": {
                "type": "string",
                "pattern": "^DOC[0-9]{3}$"
              },
              "guid": {
                "type": "uuidstring",
                "pattern": "^[0-9a-fA-F]{8}(-[0-9a-fA-F]{4}){3}-[0-9a-fA-F]{12}$"
              },
              "detail": {
                "type": "string",
                "minLength": 0,
                "maxLength": 1000
              },
              "location": {
                "type": "string",
                "minLength": 0,
                "maxLength": 1000
              },
              "updated_at": {
                "type": "string",
                "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2}.[0-9]{3}Z$"
              },
              "uploaded_pdf": {
                "type": "object",
                "properties": {
                  "description": {
                    "type": "string",
                    "minLength": 0,
                    "maxLength": 1000
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
